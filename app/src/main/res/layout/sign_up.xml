<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F8F7FBF9">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="96dp"
        android:layout_height="38dp"
        android:password="false"
        android:phoneNumber="false"
        android:text="Register"
        android:textColor="#373030"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="Deprecated,HardcodedText,MissingConstraints,TextViewEdits,TextSizeCheck"
        tools:layout_editor_absoluteY="64dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/glowify_brand_logo_design_2"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_editor_absoluteY="136dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Please enter your details to register"
        android:textColor="#555555"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteY="334dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:allowUndo="true"
        android:text="CREATE ACCOUNT"
        android:textColor="#121111"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteY="296dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:text="First Name"
        android:textColor="#555555"
        android:textSize="16sp"
        tools:ignore="HardcodedText,MissingConstraints,VisualLintOverlap"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="377dp" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:text="Email Address"
        android:textColor="#555555"
        android:textSize="16sp"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="552dp" />

    <Button
        android:id="@+id/button10"
        android:layout_width="336dp"
        android:layout_height="49dp"
        android:autoLink="phone"
        android:backgroundTint="#49FFFFFF"
        android:phoneNumber="true"
        android:text="       7xxxxxxxx"
        android:textAlignment="textStart"
        android:textColor="#703A3737"
        android:textColorLink="#3C3636"
        app:strokeColor="#44373131"
        app:strokeWidth="1dp"
        tools:ignore="Deprecated,HardcodedText,MissingConstraints,TextContrastCheck,TextViewEdits,VisualLintButtonSize"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="686dp" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:text="Mobile Number"
        android:textColor="#555555"
        android:textSize="16sp"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="660dp" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Please enter a valid mobile number (e.g., 7xxxxxxxx) "
        android:textColor="#9D332C2C"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText,MissingConstraints,TextContrastCheck,VisualLintOverlap"
        tools:layout_editor_absoluteY="735dp" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="+94 |"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="701dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="336dp"
        android:layout_height="49dp"
        android:backgroundTint="#49FFFFFF"
        android:text="Enter your last name"
        android:textAlignment="textStart"
        android:textColor="#703A3737"
        android:textColorLink="#3C3636"
        app:strokeColor="#44373131"
        app:strokeWidth="1dp"
        tools:ignore="HardcodedText,MissingConstraints,TextContrastCheck,VisualLintButtonSize,VisualLintOverlap"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="488dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="254dp"
        android:layout_height="52dp"
        android:layout_marginBottom="88dp"
        android:backgroundTint="#B87A18"
        android:insetLeft="0dp"
        android:insetTop="2dp"
        android:insetBottom="2dp"
        android:text="Register"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText,MissingConstraints,VisualLintButtonSize,DuplicateSpeakableTextCheck" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Already have an account?"
        android:textColor="#9D332C2C"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.382"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText,MissingConstraints,TextContrastCheck,VisualLintBounds"
        tools:layout_editor_absoluteY="826dp" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="_____________________________________________________"
        android:textColor="#BFBDBD"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText,MissingConstraints,VisualLintOverlap,VisualLintBounds,TextContrastCheck"
        tools:layout_editor_absoluteY="846dp" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sign In"
        android:textColor="#C88A2F"
        android:textStyle="bold"
        tools:ignore="HardcodedText,MissingConstraints,TextContrastCheck,VisualLintOverlap,VisualLintBounds"
        tools:layout_editor_absoluteX="264dp"
        tools:layout_editor_absoluteY="827dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:text="Last Name"
        android:textColor="#555555"
        android:textSize="16sp"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="462dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Please enter a valid email address."
        android:textColor="#9D332C2C"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.175"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText,MissingConstraints,TextContrastCheck"
        tools:layout_editor_absoluteY="627dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="336dp"
        android:layout_height="49dp"
        android:layout_marginStart="32dp"
        android:autoLink="email"
        android:backgroundTint="#49FFFFFF"
        android:text="Enter your email address"
        android:textAlignment="textStart"
        android:textColor="#703A3737"
        android:textColorLink="#3C3636"
        app:layout_constraintStart_toStartOf="parent"
        app:strokeColor="#44373131"
        app:strokeWidth="1dp"
        tools:ignore="HardcodedText,MissingConstraints,TextContrastCheck,VisualLintButtonSize"
        tools:layout_editor_absoluteY="578dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="336dp"
        android:layout_height="49dp"
        android:layout_marginStart="32dp"
        android:backgroundTint="#49FFFFFF"
        android:text="Enter your first name"
        android:textAlignment="textStart"
        android:textColor="#703A3737"
        android:textColorLink="#3C3636"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.461"
        app:strokeColor="#44373131"
        app:strokeWidth="1dp"
        tools:ignore="HardcodedText,MissingConstraints,TextContrastCheck,VisualLintButtonSize" />

</androidx.constraintlayout.widget.ConstraintLayout>