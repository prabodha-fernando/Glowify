        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView46"
            android:layout_width="124dp"
            android:layout_height="29dp"
            android:foregroundTint="#E91E63"
            android:password="false"
            android:phoneNumber="false"
            android:text="Need Help?"
            android:textAlignment="center"
            android:textColor="#C88A2F"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.233"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.134"
            tools:ignore="Deprecated,HardcodedText,MissingConstraints,TextViewEdits,TextSizeCheck,TextContrastCheck" />

        <Button
            android:id="@+id/button19"
            android:layout_width="347dp"
            android:layout_height="82dp"
            android:backgroundTint="#3FC591A3"
            android:text="Send us a message and our admin team will get back to you within 24 hours."
            android:textAlignment="textStart"
            android:textColor="#C88A2F"
            android:textColorLink="#3C3636"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.176"
            app:strokeColor="#44373131"
            app:strokeWidth="1dp"
            tools:ignore="HardcodedText,MissingConstraints,TextContrastCheck,VisualLintButtonSize,TextSizeCheck" />

        <TextView
            android:id="@+id/textView45"
            android:layout_width="205dp"
            android:layout_height="36dp"
            android:password="false"
            android:phoneNumber="false"
            android:text="Contact Admin"
            android:textAlignment="center"
            android:textColor="#373030"
            android:textSize="24sp"
            android:textStyle="normal"
