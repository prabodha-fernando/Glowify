                <Button
                    android:id="@+id/button20"
                    android:layout_width="320dp"
                    android:layout_height="47dp"
                    android:backgroundTint="#49FFFFFF"
                    android:text="Enter your full name"
                    android:textAlignment="textStart"
                    android:textColor="#703A3737"
                    android:textColorLink="#3C3636"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.571"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.097"
                    app:strokeColor="#44373131"
                    app:strokeWidth="1dp"
                    tools:ignore="HardcodedText,MissingConstraints,TextContrastCheck,VisualLintButtonSize,TouchTargetSizeCheck" />

                <TextView
                    android:id="@+id/textView49"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#FFFFFF"
